{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev-ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "pull-components": "sh storyblok/cli/pull-components.sh",
    "push-components": "sh storyblok/cli/push-components.sh",
    "generate-migration": "sh storyblok/cli/generate-migration.sh",
    "generate-types": "sh storyblok/cli/generate-types.sh",
    "migration": "storyblok generate-migration --space xxx -c site-config"
  },
  "dependencies": {
    "@nuxt/image": "^1.10.0",
    "@storyblok/nuxt": "^6.2.2",
    "@tailwindcss/vite": "^4.1.10",
    "@vee-validate/rules": "^4.15.1",
    "@pinia/nuxt": "^0.11.2",
    "@kgierke/nuxt-basic-auth": "^1.7.0",
    "@nuxtjs/i18n": "^10.1.1",
    "@vueuse/core": "^13.7.0",
    "@vee-validate/yup": "^4.15.0",
    "flowbite": "^3.1.2",
    "nuxt": "3.16.0",
    "tailwindcss": "^4.1.10",
    "vee-validate": "^4.15.1",
    "vue": "latest",
    "vue-router": "latest",
    "nuxt-seo-utils": "^7.0.18",
    "nuxt-swiper": "^2.0.1",
    "pinia": "^3.0.3"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.1.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "^14.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3"
  }
}
